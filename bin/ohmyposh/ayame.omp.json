{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#ff9bff",
          "background": "#763b73",
          "leading_diamond": "",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}"
        },
        {
          "type": "session",
          "style": "diamond",
          "powerline_symbol": "",
          "foreground": "#ff9bff",
          "background": "#763b73",
          "trailing_diamond": "",
          "template": " {{ .HostName }} <#ff9bff> {{if .Root}} {{.UserName}} {{else}}{{.UserName}} {{end}}"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "powerline_symbol": "",
          "foreground": "#b097ce",
          "background": "#322145",
          "template": "  {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          },
          "trailing_diamond": "",
          "leading_diamond": "<transparent,#342950></>"
        },
        {
          "type": "path",
          "foreground": "#76c3ff",
          "background": "#224a6d",
          "powerline_symbol": "",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": " <#130f1e></> ",
            "home_icon": "󰜥 ",
            "style": "agnoster_full"
          },
          "style": "powerline",
          "template": " {{ .Path }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ff9bff",
          "background": "#763b73",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#224a6d{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#79452c{{ end }}",
            "{{ if gt .Ahead 0 }}#6f2332{{ end }}",
            "{{ if gt .Behind 0 }}#587b4a{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "foreground": "#437e86",
          "background": "#93ffff",
          "background_templates": [
            "{{ if gt .Code 0 }}#ff4b73{{ end }}"
          ],
          "trailing_diamond": "",
          "template": "  ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "alignment": "right",
      "type": "prompt",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "foreground": "#9bffce",
          "background": "#4a8964",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " 󰃫 {{ .Env.keki_cake }} <#130f1e></> 󰌨 {{ .Env.keki_layers }} <#130f1e></> {{ .Env.keki_layer }} "
        },
        {
          "type": "text",
          "style": "plain",
          "template": "\u00a0\u00a0"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#b097ce",
          "background": "#322145",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#6f2332",
          "background": "#ff7e97",
          "template": ""
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#b38fff",
          "background": "#3c2865",
          "template": "  {{ .Name }} {{ .Env.POSH_INSTALER }}"
        },
        {
          "type": "text",
          "style": "plain",
          "template": "\u00a0"
        }
      ],
      "newline": true
    }
  ]
}
